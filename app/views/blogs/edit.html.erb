<div class="container">
  <div class="box col-10">
    <h2>Edit Your Blog Post</h2>
    <%= simple_form_for @blog, url: subblog_blog_path do |f|%>
      <%= f.input :title %>
      <br/>
      <%= f.input :content %>
      <%= f.submit 'Post Edit', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
